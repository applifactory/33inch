.inch33
  font-size: 18px
  font-family: 'Raleway', sans-serif
  color: #3a4553
  h1, h2, h3, h4, h5
    font-family: 'Arvo', serif
  h1
    font-weight: normal
    font-size: 2.5em
    margin: 0.5em 0
  h2
    font-weight: 400
    font-size: 2em
    margin: 0.5em 0
  p
    line-height: 1.75
    &.claim
      font-size: 1.5em
